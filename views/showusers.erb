

<table class="table table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th>Date Stamp</th>
    <th>Barber</th>
    <th>Color</th>
  </tr>
  </thead>

  <tbody>
  <% @results_users.each do |row| %>
      <tr>
        <td><%= row['name'] %></td>
        <td><%= row['phone'] %></td>
        <td><%= row['dataStamp'] %></td>
        <td><%= row['Barber'] %></td>
        <td style="background-color: <%= row['color'] %>"><%= row['color'] %></td>
      </tr>
      </tbody>
  <% end %>
  </table>


